<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns="http://hpxml.org/hpxml/2011/1"
	targetNamespace="http://hpxml.org/hpxml/2011/1" elementFormDefault="unqualified"
	attributeFormDefault="unqualified">
	<xs:include schemaLocation="HPXMLDataTypes.xsd"/>
	<xs:import/>
	<xs:element name="XMLTransactionHeaderInformation">
		<xs:annotation>
			<xs:documentation>These are a series of elements that indicate the type of XML and basic descriptive data about the XML.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="XMLType" type="XMLType"/>
				<xs:element name="XMLGeneratedBy" type="XMLGeneratedBy"/>
				<xs:element name="CreatedDateAndTime" type="CreatedDateAndTime"/>
				<xs:element name="Transaction" type="TransactionType"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:group name="AddressInformation">
		<xs:annotation>
			<xs:documentation>Address information contains the basic descriptive elements of a location.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="AddressTypeCode" type="AddressTypeCode"/>
			<xs:element name="Address1" type="Address1" minOccurs="0"/>
			<xs:element name="Address2" type="Address2" minOccurs="0"/>
			<xs:element name="CityMunicipality" type="CityMunicipality"/>
			<xs:element name="StateCode" type="StateCode"/>
			<xs:element name="ZipCodePlus4" type="ZipCodePlus4"/>
			<xs:element name="USPSBarCode" type="USPSBarCode" minOccurs="0"/>
			<xs:group ref="ExtensibilityGroup"/>
		</xs:sequence>
	</xs:group>
	<xs:complexType name="SystemIdentifiersInfoType">
		<xs:annotation>
			<xs:documentation>System identifiers contain type codes and an identifier for both a sending and a receiving system.  These fields are needed to be able to transmit data between two systems, and have it identified in the two systems.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="SendingSystemIdentifierType" type="SendingSystemIdentifierType"/>
			<xs:element name="SendingSystemIdentifierValue" type="SendingSystemIdentifierValue"/>
			<xs:element name="ReceivingSystemIdentifierType" type="ReceivingSystemIdentifierType"
				minOccurs="0"/>
			<xs:element name="ReceivingSystemIdentifierValue" type="ReceivingSystemIdentifierValue"
				minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<xs:element name="AddressSystemIdentifiers" type="SystemIdentifiersInfoType">
		<xs:annotation>
			<xs:documentation>Most HPXML records contain data about a specfic address, or location.  This element contains the root elements for address identifier values between a sending and a receiving system.</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="IndividualSystemIdentifiers" type="SystemIdentifiersInfoType">
		<xs:annotation>
			<xs:documentation>HPXML records may contain data about an individual, either a person, or a business.  This element contains the root elements for individual identifier values between a sending and a receiving system.</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="ContractorSystemIdentifiers" type="SystemIdentifiersInfoType">
		<xs:annotation>
			<xs:documentation>These are the system identifiers for a specific contractor at a business</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="MiscDataFields">
		<xs:annotation>
			<xs:documentation>These attributes allow for the addition of undefined data fields to any attribute group.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence minOccurs="0" maxOccurs="unbounded">
				<xs:element name="FieldSequenceNumber" type="FieldSequenceNumber"/>
				<xs:element name="FieldName" type="FieldName"/>
				<xs:element name="FieldDataType" type="FieldDataType"/>
				<xs:element name="FieldValue" type="FieldValue"/>
				<xs:element name="FieldMask" type="FieldMask" minOccurs="0"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:group name="ExtensibilityGroup">
		<xs:sequence>
			<xs:element ref="MiscDataFields" minOccurs="0"/>
			<xs:any namespace="##other" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:group>
	<xs:group name="StandardMeasureFieldsGroup">
		<xs:sequence>
			<xs:element name="ImplementationCost" minOccurs="0" type="xs:double"/>
			<xs:element name="AdditionalInformation" minOccurs="0" type="xs:string"/>
			<xs:element ref="ContractorSystemIdentifiers" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="CompletionDate" type="xs:date" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Date measure was completed</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element ref="EnergySavingsInfo" minOccurs="0"/>
			<xs:group ref="ExtensibilityGroup"/>
		</xs:sequence>
	</xs:group>
	<xs:element name="TelephoneInfo">
		<xs:annotation>
			<xs:documentation>Contains all telephone elements.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="TelephoneTypeCode" type="TelephoneTypeCode"/>
				<xs:element name="TelephoneNumber" type="TelephoneNumber"/>
				<xs:element name="TelephoneExtension" type="TelephoneExtension" minOccurs="0"/>
				<xs:group ref="ExtensibilityGroup"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="EmailInfo">
		<xs:annotation>
			<xs:documentation>Contains all email elements.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence minOccurs="0" maxOccurs="unbounded">
				<xs:element name="EmailTypeCode" type="EmailTypeCode"/>
				<xs:element name="EmailAddress" type="EmailAddress"/>
				<xs:group ref="ExtensibilityGroup"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="IndividualNameInfo">
		<xs:annotation>
			<xs:documentation>Contains all name elements for either an individual or a business.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="NameTypeCode" type="NameTypeCode"/>
				<xs:element name="FirstName" type="FirstName"/>
				<xs:element name="LastName" type="LastName"/>
				<xs:element name="MiddleName" type="MiddleName" minOccurs="0"/>
				<xs:element name="PrefixName" type="PrefixName" minOccurs="0"/>
				<xs:element name="SuffixName" type="SuffixName" minOccurs="0"/>
				<xs:group ref="ExtensibilityGroup"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:group name="IndividualInfo">
		<xs:sequence>
			<xs:element ref="IndividualSystemIdentifiers"/>
			<xs:element ref="IndividualNameInfo"/>
			<xs:element ref="TelephoneInfo" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element ref="EmailInfo" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="IndividualTypeCode" type="IndividualTypeCode"/>
			<xs:group ref="ExtensibilityGroup"/>
		</xs:sequence>
	</xs:group>
	<xs:element name="UtilityInfo">
		<xs:annotation>
			<xs:documentation>Contains all utility elements.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="UtilityIdentifiers" type="SystemIdentifiersInfoType"/>
				<xs:element name="UtilityAccountNumber" type="UtilityAccountNumber"/>
				<xs:element name="UtilityName" type="UtilityName" minOccurs="0"/>
				<xs:element name="UtilityServiceTypeProvided" type="UtilityServiceTypeProvided"
					minOccurs="0"/>
				<xs:element ref="BusinessInfo" minOccurs="0"/>
				<xs:element ref="BusinessContactInfo" minOccurs="0" maxOccurs="unbounded"/>
				<xs:group ref="ExtensibilityGroup"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="BusinessInfo">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="IndividualSystemIdentifiers"/>
				<xs:element ref="BusinessNameInfo"/>
				<xs:element name="BusinessSpecialization" type="BusinessSpecialization"
					minOccurs="0"/>
				<xs:element name="BusinessType" type="BusinessType" minOccurs="0"/>
				<xs:element ref="EmailInfo" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="TelephoneInfo" minOccurs="0" maxOccurs="unbounded"/>
				<xs:group ref="ExtensibilityGroup"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="BusinessContactInfo">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="ContactType" type="ContactType" minOccurs="0"/>
				<xs:element name="FirstName" type="FirstName" minOccurs="0"/>
				<xs:element name="LastName" type="LastName" minOccurs="0"/>
				<xs:group ref="ExtensibilityGroup"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="BusinessNameInfo">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="NameTypeCode" type="NameTypeCode"/>
				<xs:element name="BusinessName" type="BusinessName"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="AirInfiltrationMeasurement">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="BuildingAirLeakage" type="BuildingAirLeakage" minOccurs="0"/>
				<xs:element name="BuildingAirLeakageUnit" type="BuildingAirLeakageUnit"
					minOccurs="0"/>
				<xs:element name="HousePressure" type="xs:double"/>
				<xs:element name="FanPressure" type="FanPressure" minOccurs="0"/>
				<xs:element name="FanRingUsed" type="FanRingUsed" minOccurs="0"/>
				<xs:element name="TypeofTest" type="TypeofTest" minOccurs="0"/>
				<xs:group ref="ExtensibilityGroup" minOccurs="0"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="ClimateandRiskZonesInfo">
		<xs:complexType>
			<xs:sequence minOccurs="0">
				<xs:element name="ClimateZoneDOE" type="ClimateZoneDOE" minOccurs="0"/>
				<xs:element name="ClimateZoneIECC" type="ClimateZoneIECC" minOccurs="0"/>
				<xs:element name="EarthquakeZone" type="EarthquakeZone" minOccurs="0"/>
				<xs:element name="RadonZone" type="RadonZone" minOccurs="0"/>
				<xs:element name="TermiteZone" type="TermiteZone" minOccurs="0"/>
				<xs:element minOccurs="0" name="HurricaneZone" type="xs:boolean"/>
				<xs:element minOccurs="0" name="FloodZone" type="xs:boolean"/>
				<xs:group ref="ExtensibilityGroup" minOccurs="0"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="ConstructionInfo">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="YearBuilt" type="YearBuilt" minOccurs="0"/>
				<xs:element name="Hometype" type="Hometype" minOccurs="0"/>
				<xs:element name="NumberofUnitsRetrofit" type="NumberofUnitsRetrofit" minOccurs="0"/>
				<xs:element name="NumberofOccupants" type="NumberofOccupants" minOccurs="0"/>
				<xs:element name="NumberofBedrooms" type="NumberofBedrooms" minOccurs="0"/>
				<xs:element name="NumberofBathrooms" type="NumberofBedrooms" minOccurs="0"/>
				<xs:element name="NumberofStoriesAboveGrade" type="NumberofStoriesAboveGrade"
					minOccurs="0"/>
				<xs:element name="AverageCeilingHeight" type="AverageCeilingHeightType"
					minOccurs="0"/>
				<xs:element name="ConditionedFloorArea" type="ConditionedFloorArea" minOccurs="0"/>
				<xs:element name="ConditionedVolume" type="ConditionedVolume" minOccurs="0"/>
				<xs:element name="FoundationType" type="FoundationType" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Primary foundation type of building</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="ShieldingofHome" type="ShieldingofHome" minOccurs="0"/>
				<xs:group ref="ExtensibilityGroup" minOccurs="0"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="ConsumptionInfo">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="UtilityID" type="SystemIdentifiersInfoType"/>
				<xs:element name="ConsumptionType" type="ConsumptionType">
					<xs:annotation>
						<xs:documentation>Energy Type + other consumption types like water</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="UnitofMeasure" type="UnitofMeasure"/>
				<xs:element ref="ConsumptionDetailInfo" maxOccurs="unbounded" minOccurs="1">
					<xs:annotation>
						<xs:documentation>Consumption records with enough granularity to be able to use smart meter data.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element minOccurs="0" name="MarginalRate" type="xs:double"/>
				<xs:group ref="ExtensibilityGroup" minOccurs="0"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="ConsumptionDetailInfo">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="EnergyConsumption" type="EnergyConsumption"/>
				<xs:element name="StartDateTime" type="StartDateTime" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Date/time stamp in the ISO 8601 format when the usage measured began. Set time to midnight if time is not known.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="EndDateTime" type="EndDateTime">
					<xs:annotation>
						<xs:documentation>Date/time stamp of the meter reading.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element minOccurs="0" name="ConsumptionCost" type="xs:double"/>
				<xs:group ref="ExtensibilityGroup" minOccurs="0"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="StoveTest">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="Fuel" type="HeatingStoveFuel" minOccurs="0"/>
				<xs:element name="COReading" type="COReading" minOccurs="0"/>
				<xs:element name="GasLeaksIdentified" type="GasLeaksIdentified" minOccurs="0"/>
				<xs:element name="HeatingStoveProperlyVented" type="HeatingStoveProperlyVented"
					minOccurs="0"/>
				<xs:element name="ActionsTaken" minOccurs="0" type="xs:string"/>
				<xs:group ref="ExtensibilityGroup" minOccurs="0"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="OtherHealthandSafetyInfo">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="ExtDisturbanceExceed6SQFTforPre1978"
					type="ExtDisturbanceExceed6SQFTforPre1978" minOccurs="0"/>
				<xs:element name="OneCODetectorPerFloorMeetsLocalCode"
					type="OneCODetectorPerFloorMeetsLocalCode" minOccurs="0"/>
				<xs:element name="OtherActionsTakenDescription" type="OtherActionsTakenDescription"
					minOccurs="0"/>
				<xs:element name="OtherHealthandSafetyIdentifier"
					type="OtherHealthandSafetyIdentifier" minOccurs="0"/>
				<xs:element minOccurs="0" name="RadonTested" type="xs:boolean"/>
				<xs:element name="RadonTestResults" type="RadonTestResults" minOccurs="0"/>
				<xs:element name="SmokeAlarmsMeetLocalCode" type="SmokeAlarmsMeetLocalCode"
					minOccurs="0"/>
				<xs:element name="OtherHealthandSafetyIssues" type="xs:string" minOccurs="0"/>
				<xs:group ref="ExtensibilityGroup" minOccurs="0"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="AirSealingInfo">
		<xs:complexType>
			<xs:sequence>
				<xs:group ref="SystemInfo"/>
				<xs:group ref="StandardMeasureFieldsGroup" minOccurs="0"/>
				<xs:element name="OtherAreasDescription" type="OtherAreasDescription" minOccurs="0"/>
				<xs:element name="BuildingAirLeakage" type="BuildingAirLeakage"/>
				<xs:element name="BuildingAirLeakageUnit" type="BuildingAirLeakageUnit"/>
				<xs:group ref="ExtensibilityGroup" minOccurs="0"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="DoorsInfo">
		<xs:complexType>
			<xs:sequence>
				<xs:group ref="SystemInfo"/>
				<xs:element name="ExistingStormDoor" type="ExistingStormDoor" minOccurs="0"/>
				<xs:element name="PreRetrofitDescription" type="PreRetrofitDescription"
					minOccurs="0"/>
				<xs:element minOccurs="0" maxOccurs="unbounded" name="DoorImprovementInfo">
					<xs:complexType>
						<xs:sequence>
							<xs:group ref="StandardMeasureFieldsGroup" minOccurs="0"/>
							<xs:element name="ReplacementDescription" type="ReplacementDescription"
								minOccurs="0"/>
							<xs:element name="ReplacementRValue" type="ReplacementRValue"
								minOccurs="0"/>
							<xs:element name="StormDoorInstalled" type="StormDoorInstalled"
								minOccurs="0"/>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:group ref="ExtensibilityGroup" minOccurs="0"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="InsulationInfo">
		<xs:complexType>
			<xs:sequence>
				<xs:group ref="SystemInfo"/>
				<xs:sequence maxOccurs="unbounded">
					<xs:element name="AreaInsulated" type="AreaInsulatedType"/>
					<xs:element name="LayerNumber" type="LayerNumber" minOccurs="0"/>
					<xs:element name="InstallationType" type="InstallationType" minOccurs="0"> </xs:element>
					<xs:element name="InsulationMaterial" type="InsulationMaterial" minOccurs="0"/>
					<xs:element name="NominalRValue" type="NominalRValue" minOccurs="0"/>
					<xs:element name="Thickness" type="Thickness" minOccurs="0"/>
					<xs:group ref="ExtensibilityGroup" minOccurs="0"/>
				</xs:sequence>
				<xs:element minOccurs="0" maxOccurs="unbounded" name="InsulationImprovementInfo">
					<xs:complexType>
						<xs:sequence>
							<xs:group ref="StandardMeasureFieldsGroup"/>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="LightingInfo">
		<xs:complexType>
			<xs:sequence>
				<xs:group ref="SystemInfo"/>
				<xs:element name="AverageWattage" minOccurs="0" type="xs:double"/>
				<xs:element name="AverageHoursPerDay" minOccurs="0" type="xs:double"/>
				<xs:element name="NumberofUnits" minOccurs="0" type="xs:integer"/>
				<xs:group ref="ExtensibilityGroup" minOccurs="0"/>
				<xs:element maxOccurs="unbounded" minOccurs="0" name="LightingImprovementInfo">
					<xs:complexType>
						<xs:sequence>
							<xs:group ref="StandardMeasureFieldsGroup" minOccurs="0"/>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="LightingControlsInfo">
		<xs:complexType>
			<xs:sequence>
				<xs:group ref="SystemInfo"/>
				<xs:group ref="ExtensibilityGroup" minOccurs="0"/>
				<xs:element name="LightingControlsImprovmentInfo" maxOccurs="unbounded"
					minOccurs="0">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="Type" type="TypeofReplacement" minOccurs="0">
								<xs:annotation>
									<xs:documentation>occupancy sensor, photo sensor, timer</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:group ref="StandardMeasureFieldsGroup" minOccurs="0"/>
							<xs:element name="NumberofLightingControls"
								type="NumberofReplacementsInstalled" minOccurs="0"/>
							<xs:element name="Location" type="LocationOfReplacement" minOccurs="0">
								<xs:annotation>
									<xs:documentation>interior, exterior</xs:documentation>
								</xs:annotation>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="MoistureControlInfo">
		<xs:complexType>
			<xs:sequence>
				<xs:group ref="SystemInfo"/>
				<xs:element name="ExteriorLocationsWaterIntrusionorDamage"
					type="ExteriorLocationsWaterIntrusionorDamage" minOccurs="0"/>
				<xs:element name="InteriorLocationsofWaterLeaksorDamage"
					type="InteriorLocationsofWaterLeaksorDamage" minOccurs="0"/>
				<xs:element minOccurs="0" name="MoistureControlImprovementInfo"
					maxOccurs="unbounded">
					<xs:complexType>
						<xs:sequence>
							<xs:group ref="StandardMeasureFieldsGroup" minOccurs="0"/>
							<xs:element name="GuttersInstalledorRepaired"
								type="GuttersInstalledorRepaired" minOccurs="0"/>
							<xs:element name="FlashingInstalledorRepaired"
								type="FlashingInstalledorRepaired" minOccurs="0"/>
							<xs:element name="FoundationGradingImproved"
								type="FoundationGradingImproved" minOccurs="0"/>
							<xs:element name="OtherMeasuresImplementedDescription"
								type="OtherMeasuresImplementedDescription" minOccurs="0"/>
							<xs:element name="VaporRetardersInstalled"
								type="VaporRetardersInstalled" minOccurs="0"/>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:group ref="ExtensibilityGroup" minOccurs="0"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="PlugLoadControlInfo">
		<xs:complexType>
			<xs:sequence>
				<xs:group ref="SystemInfo"/>
				<xs:group ref="ExtensibilityGroup" minOccurs="0"/>
				<xs:element minOccurs="0" name="PlugLoadControlImprovementInfo"
					maxOccurs="unbounded">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="NumberInstalled" type="NumberInstalled" minOccurs="0"/>
							<xs:group ref="StandardMeasureFieldsGroup" minOccurs="0"/>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="ShadingAndSolarReflectanceInfo">
		<xs:complexType>
			<xs:sequence>
				<xs:group ref="SystemInfo"/>
				<xs:element name="RadiantBarrier" type="RadiantBarrier" minOccurs="0"/>
				<xs:element name="RadiantBarrierLocation" type="RadiantBarrierLocation"
					minOccurs="0"/>
				<xs:group ref="ExtensibilityGroup" minOccurs="0"/>
				<xs:element minOccurs="0" maxOccurs="unbounded"
					name="ShadingAndSolarReflectanceImprovementInfo">
					<xs:complexType>
						<xs:sequence>
							<xs:group ref="StandardMeasureFieldsGroup"/>
							<xs:element name="OtherShadingandSolarMeasuresInstalled"
								type="OtherShadingandSolarMeasuresInstalled" minOccurs="0"/>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="VentilationInfo">
		<xs:complexType>
			<xs:sequence>
				<xs:group ref="SystemInfo"/>
				<xs:element name="MechanicalVentilationDetermination"
					type="MechanicalVentilationDetermination" minOccurs="0"/>
				<xs:element name="MVRatCFM50" type="MVRatCFM50" minOccurs="0"/>
				<xs:element name="MVRDeterminationMethod" type="MVRDeterminationMethod"
					minOccurs="0"/>
				<xs:element minOccurs="0" name="MVRRecommendation" type="MVRRecommendation"/>
				<xs:element name="OtherVentilationIssuesDescription"
					type="OtherVentilationIssuesDescription" minOccurs="0"/>
				<xs:element name="AirLeakage" type="BuildingAirLeakage" minOccurs="0"/>
				<xs:element name="ProperClothersDryerVenting" type="ProperClothersDryerVenting"
					minOccurs="0"/>
